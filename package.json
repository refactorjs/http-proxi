{
    "name": "@refactorjs/http-proxy",
    "version": "0.0.6",
    "description": "http-proxy alternative",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/refactorjs/http-proxy.git"
    },
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "tscbuild": "tsc -p ./tsconfig.json",
        "release": "standard-version && git push --follow-tags && npm publish",
        "test": "vitest run"
    },
    "dependencies": {
        "eventemitter3": "^4.0.0",
        "follow-redirects": "^1.15.1",
        "requires-port": "^1.0.0"
    },
    "devDependencies": {
        "@types/follow-redirects": "^1.14.1",
        "@types/node": "^18.0.0",
        "@types/requires-port": "^1.0.0",
        "async": "^3.0.0",
        "concat-stream": "^2.0.0",
        "nyc": "^15.0.0",
        "semver": "^7.3.7",
        "socket.io": "^4.5.1",
        "socket.io-client": "^4.5.1",
        "sse": "0.0.8",
        "standard-version": "latest",
        "typescript": "^4.7.4",
        "unbuild": "^0.8.8",
        "vitest": "^0.21.1",
        "ws": "^8.0.0"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "yarn@3.2.0",
    "bugs": {
        "url": "https://github.com/refactorjs/http-proxy/issues"
    },
    "homepage": "https://github.com/refactorjs/http-proxy#readme",
    "keywords": [
        "http-proxy",
        "node-http-proxy",
        "http-proxy",
        "alternative"
    ]
}
